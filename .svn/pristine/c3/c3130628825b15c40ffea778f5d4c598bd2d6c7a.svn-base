.coupon-container {
  background-color: #cb1402;
  background-repeat: no-repeat;
  background-size: contain;
  padding-top: 450rpx;
  box-sizing: border-box;
}
.coupon-container .empty-box {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  width: 80%;
  margin: 0 auto;
  padding: 20rpx;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
}
.coupon-container .empty-box > image {
  width: 200rpx;
  height: 200rpx;
}
.coupon-container .empty-box > view {
  margin-top: 40rpx;
  font-size: 26rpx;
  font-weight: 500;
  color: #ffffff;
  opacity: 0.9;
}
.coupon-container .coupon-bottom {
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 100rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  background-color: white;
}
.coupon-container .coupon-bottom > image {
  width: 32rpx;
  height: 32rpx;
}
.coupon-container .coupon-bottom > text {
  font-size: 29rpx;
  font-weight: 700;
  color: #000000;
}
