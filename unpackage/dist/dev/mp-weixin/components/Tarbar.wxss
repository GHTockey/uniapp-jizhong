.tabbar {
  height: 95.83rpx;
  background: #ffffff;
  display: flex;
  justify-content: space-around;
  align-items: center;
  background-color: #fff;
  border-top: 1px solid #eaeaea;
  padding: 16rpx 0;
  box-sizing: border-box;
}
.tabbar .tabbar-item {
  display: flex;
  flex-direction: column;
  align-items: center;
}
.tabbar .tabbar-item .tabbar-icon {
  width: 37.5rpx;
  height: 37.5rpx;
}
.tabbar .tabbar-item .tabbar-text {
  margin-top: 4rpx;
  color: #666;
  font-size: 19.44rpx;
  font-family: PingFang SC, PingFang SC-Bold;
  font-weight: 700;
}
.tabbar .tabbar-item .tabbar-text.active {
  color: red;
  /* 高亮颜色 */
}
